}

model nick_history {
  id          Int     @id @default(autoincrement())
  nick        String? @db.VarChar(40)
  jogador_id  Int?
  nick_antigo String? @db.VarChar(255)
  data        String? @db.VarChar(75)
}

model objects {
  id        Int    @id @default(autoincrement())
  modelid   Int    @default(7899)
  x         Float? @default(0) @db.Float
  y         Float? @default(0) @db.Float
  z         Float? @default(0) @db.Float
  rx        Float? @default(0) @db.Float
  ry        Float? @default(0) @db.Float
  rz        Float? @default(0) @db.Float
  distance  Float? @default(0) @db.Float
  player_id Int
  map_id    Int
  virtualw  Int
  inter     Int
  status    Int?   @default(1)
}

