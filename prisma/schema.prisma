  SPAWN_LAST        Int         @default(0)
  Warnings          Int         @default(0)
  MUTED             Boolean     @default(false)
  KitRun            Boolean     @default(false)
  KitWalk           Boolean     @default(true)
  Version           String      @default("3.0.1") @db.VarChar(24)
  Device            String      @default("Nao Verificado") @db.VarChar(24)
  Tag               String      @default("Grauzinho") @db.VarChar(255)
  Tag_Color         String      @default("00FF00") @db.VarChar(255)
  Tag_Color_hexa    String      @default("0x00FF00") @db.VarChar(255)
  Tag_Inuse         Boolean     @default(true)
  YouTuber          Boolean     @default(false)
  PLYAERHASTAG      Boolean     @default(true)
  TimerOnServer     Int         @default(1)
  MODO_MATA         Boolean     @default(true)
  Email_confirmed   Boolean     @default(false)
  spawn_colete      Boolean     @default(false)
  discord_id        String      @default("nenhum") @db.VarChar(255)
  discord_codsync   String      @default("nenhum") @db.VarChar(255)
