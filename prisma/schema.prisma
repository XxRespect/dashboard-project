  cidade            String      @default("Nao Definido") @db.VarChar(255)
  regiao            String      @default("Nao Definido") @db.VarChar(255)
  cep               String      @default("Nao Definido") @db.VarChar(255)
  organizacao       String      @default("Nao Definido") @db.VarChar(255)
  fuso_horario      String      @default("Nao Definido") @db.VarChar(255)
  isp               String      @default("Nao Definido") @db.VarChar(255)
  vpn               String      @default("Privado") @db.VarChar(255)
  coordenadas       String      @default("Nao definido") @db.VarChar(255)
  StoragedNick      Boolean     @default(false)
  grupo             String      @default("semgrupo") @db.VarChar(100)
