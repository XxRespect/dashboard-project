  Dinheiro          BigInt      @default(3000)
  HeadShots         Int         @default(1)
  pPosX             Float?      @default(-2240.9197)
  pPosY             Float?      @default(252.0263)
  pPosZ             Float?      @default(35.3203)
  pPosA             Float?      @default(91.2125)
