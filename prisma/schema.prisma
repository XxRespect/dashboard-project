  Arena_deaths      Int         @default(0)
  Arena_scores      Int         @default(0)
  Hud_players       Boolean     @default(true)
  blockban          Boolean     @default(false)
  Mapper            Boolean     @default(false)
  isVpn             Boolean     @default(false)
  banreason         String?     @default("Jogador nao banido") @db.VarChar(255)
